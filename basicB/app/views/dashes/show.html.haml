%p#notice= notice

%p
  %b Name:
  = @dash.name
%p
  %b User:
  = @dash.user_id
%p
  %b Dashcampaigns:
  = @dash.dashcampaigns.name
%p
  %b Dashcampaigns: 
  %ul
  - if @dash.campaigns.count > 0 then
    - @dash.campaigns.each do |x|
      %li= x.id
      %li= x.name
  - else
    Nothing to see here
%p
  %b Dashplayers:
  = @dash.dashplayers

= link_to 'Edit', edit_dash_path(@dash)
\|
= link_to 'Back', dashes_path
