%table{:class => "table table-striped table-hover"}
  %thead
    %tr
      %th Name
      %th User
      %th Dashcampaigns
      %th Dashplayers
      %th
      %th
      %th

  %tbody
    - @dashes_list.each do |dash|
      %tr
        %td= dash.name
        // %td= dash.user_id
        %td= dash.dashcampaigns.name
        %td= dash.dashplayers.name
        %td= link_to 'Show', dash
        %td= link_to 'Edit', edit_dash_path(dash)
        %td= link_to 'Destroy', dash, :method => :delete, :data => { :confirm => 'Are you sure?' }
= will_paginate @dashes_list